{
    "name": "TrainEffectiveApp",
    "version": "2.0.4",
    "private": true,
    "scripts": {
        "postinstall": "npx jetify && patch-package",
        "lint": "eslint src",
        "preinstall:pods": "cd ios",
        "install:pods": "pod install",
        "postinstall:pods": "cd ..",
        "clean:pods": "rimraf ios/Pods ios/Podfile.lock",
        "prerefresh:pods": "yarn clean:pods",
        "refresh:pods": "yarn install:pods",
        "preclean": "yarn cache clean",
        "clean": "rm -rf node_modules package-lock.json yarn.lock yarn-error.log && cd ios && rm -rf Pods Podfile.lock && cd .. && cd android && rm -rf build && cd ..",
        "postclean": "watchman watch-del-all && yarn && cd ios && pod repo update && pod install && cd ..",
        "prerefresh": "yarn clean",
        "refresh": "yarn install",
        "start": "react-native start",
        "ios": "react-native run-ios",
        "android": "react-native run-android",
        "preandroid:release": "./android/gradlew clean",
        "android:release": "./android/gradlew assembleRelease",
        "build:prod:android": "yarn clean && sh ./updateAndroidReleaseBundle.sh && react-native run-android --variant=release",
        "build:prod:ios": "yarn clean && npx react-native run-ios --configuration Release",
        "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
        "distribute:firebase:ios": "sh ./firebaseDistributeIos.sh",
        "detox": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd .. && detox test -c android.emu.debug"
    },
    "rnpm": {
        "assets": [
            "./src/assets/fonts/"
        ]
    },
